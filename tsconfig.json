{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "components/*": ["src/components/*"],
      "modules/*": ["src/modules/*"],
      "utils/*": ["src/utils/*"],
      "stores/*": ["src/stores/*"],
      "routers/*": ["src/routers/*"],
      "assets/*": ["src/assets/*"],
      "constants/*": ["src/constants/*"],
      "types*": ["src/types/*"]
    },
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": ["src", "global.d.ts", "global.d.ts"]
}
